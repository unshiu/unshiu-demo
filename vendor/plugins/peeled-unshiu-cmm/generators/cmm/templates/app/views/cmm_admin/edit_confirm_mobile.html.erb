<% content_for(:header_title) do %>
<%= t('view.noun.cmm_community') %>編集
<% end %>

<%= render :partial => 'cmm/confirm' %>

<% form_tag :action => "edit_complete", :id => @community.id do %>
	<%= hidden_field 'community', 'id'%>
	<%= hidden_field 'community', 'name'%>
	<%= hidden_field 'community', 'profile'%>
	<%= hidden_field 'community', 'join_type'%>
	<%= hidden_field 'community', 'topic_create_level'%>
	
	<div style="text-align:center">
		<%= submit_tag '編集' %>
		<%= cancel_tag %>
	</div>
<% end %>
